# Ejemplo de configuración para extracción de API REST
# Usa JSONPlaceholder como API de prueba pública

# Database
DB_HOST=localhost
DB_PORT=3306
DB_USER=base_user
DB_PASSWORD=base_password
DB_NAME=fuentes_base

# Source type
SOURCE_TYPE=api_rest

# API URLs - JSONPlaceholder (API de prueba pública)
API_URL_1=https://jsonplaceholder.typicode.com/posts
API_URL_2=https://jsonplaceholder.typicode.com/users
API_URL_3=https://jsonplaceholder.typicode.com/comments

# HTTP Configuration
REQUEST_TIMEOUT=30
MAX_RETRIES=3

# Logging
LOG_LEVEL=INFO
