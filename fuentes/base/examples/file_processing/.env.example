# Ejemplo de configuración para descarga de archivos

# Database
DB_HOST=localhost
DB_PORT=3306
DB_USER=base_user
DB_PASSWORD=base_password
DB_NAME=fuentes_base

# Source type
SOURCE_TYPE=file_download

# File URLs (ejemplos con archivos públicos)
FILE_URL_1=https://www.w3.org/WAI/ER/tests/xhtml/testfiles/resources/pdf/dummy.pdf
FILE_URL_2=https://file-examples.com/storage/fe7c0b96c4bf935e38bf385/2017/02/file_example_XLSX_10.xlsx

# Download configuration
DOWNLOAD_DIR=downloads

# HTTP Configuration
REQUEST_TIMEOUT=60
MAX_RETRIES=3

# Logging
LOG_LEVEL=INFO
