# Ejemplo de configuración para web scraping estático

# Database
DB_HOST=localhost
DB_PORT=3306
DB_USER=base_user
DB_PASSWORD=base_password
DB_NAME=fuentes_base

# Source type
SOURCE_TYPE=web_static

# URLs para scraping (sitios de ejemplo)
WEB_URL_1=https://en.wikipedia.org/wiki/Python_(programming_language)
WEB_URL_2=http://quotes.toscrape.com/
WEB_URL_3=http://books.toscrape.com/

# HTTP Configuration
REQUEST_TIMEOUT=30
MAX_RETRIES=3

# Logging
LOG_LEVEL=INFO
