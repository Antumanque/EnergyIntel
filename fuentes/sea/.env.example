# =============================================================================
# SEA SEIA - Configuración de Ambiente
# =============================================================================

# -----------------------------------------------------------------------------
# Base de Datos
# -----------------------------------------------------------------------------
DB_HOST=sea_db
DB_PORT=3306
DB_USER=sea_user
DB_PASSWORD=sea_password
DB_NAME=sea_seia

# -----------------------------------------------------------------------------
# API del SEA
# -----------------------------------------------------------------------------
# URL base de la API de búsqueda de proyectos
SEA_API_BASE_URL=https://seia.sea.gob.cl/busqueda/buscarProyectoResumenAction.php

# Parámetros de búsqueda por defecto
# Dejar vacío para extraer todos los proyectos
SEA_NOMBRE=
SEA_TITULAR=
SEA_FOLIO=
SEA_SELECT_REGION=
SEA_SELECT_COMUNA=
SEA_TIPO_PRESENTACION=
SEA_PROJECT_STATUS=
SEA_PRESENTACION_MIN=
SEA_PRESENTACION_MAX=
SEA_CALIFICA_MIN=
SEA_CALIFICA_MAX=
SEA_SECTORES_ECONOMICOS=
SEA_RAZON_INGRESO=
SEA_ID_TIPO_EXPEDIENTE=

# Paginación
SEA_LIMIT=100                           # Número de resultados por página
SEA_ORDER_COLUMN=FECHA_PRESENTACION     # Columna para ordenar
SEA_ORDER_DIR=desc                      # Dirección de ordenamiento (asc/desc)

# -----------------------------------------------------------------------------
# HTTP Client
# -----------------------------------------------------------------------------
REQUEST_TIMEOUT=30                      # Timeout en segundos
MAX_RETRIES=3                           # Intentos máximos de retry

# -----------------------------------------------------------------------------
# Logging
# -----------------------------------------------------------------------------
LOG_LEVEL=INFO                          # DEBUG, INFO, WARNING, ERROR, CRITICAL
